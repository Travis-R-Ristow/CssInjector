{
  "manifest_version": 3,
  "name": "Hey Puppies",
  "description": "Learning Extension",
  "version": "1.0",
  "action": {
    "default_title": "Hey Puppies",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "permissions": ["scripting", "activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["pushDefaults.js"]
    }
  ]
}
